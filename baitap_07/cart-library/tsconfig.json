{
    "compilerOptions": {
        "target": "ES2017",
        "module": "ESNext",
        "jsx": "react-jsx",
        "declaration": true,
        "declarationDir": "dist",
        "outDir": "dist",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist", "example"]
}